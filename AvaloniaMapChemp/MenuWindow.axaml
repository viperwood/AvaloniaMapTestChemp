<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaMapChemp.MenuWindow"
        Title="MenuWindow">
        <Grid RowDefinitions="* * *" ColumnDefinitions="* * *">
                <StackPanel Grid.Column="1" Grid.Row="1">
                        <Button Content="Отслеживание" Click="Map1Click"/>
                        <Button Content="Список" Click="ListClick"/>
                        <Button Content="Регистрация движения пациентов по стационару и занятость коечного фонда" Click="Map2Click"/>
                </StackPanel>
        </Grid>
</Window>
